        </Literal>
        <Literal Editable="false">
          <ID>entityloadercreatorargs</ID>
          <Type></Type>
          <ToolTip></ToolTip>
          <Default></Default>
          <Function>SimpleTypeName(Colosoft.Business.EntityLoaderCreatorArgs)</Function>
        </Literal>
        <Literal Editable="false">
          <ID>IEntityTypeManager</ID>
          <Type></Type>
          <ToolTip></ToolTip>
          <Default>IEntityTypeManager</Default>
          <Function>SimpleTypeName(Colosoft.Business.IEntityTypeManager)</Function>
        </Literal>
      </Declarations>
      <Code Language="csharp" Kind="" Delimiter="$"><![CDATA[/// <summary>
		/// Construtor padrão.
		/// </summary>
		public $classname$()
			: this(null, null, null)
		{

		}

		/// <summary>
		/// Construtor padrão.
		/// </summary>
		/// <param name="args"></param>
		protected $classname$($entityloadercreatorargs$<$datamodeltype$> args)
			: base(args.DataModel, args.UIContext, args.TypeManager)
		{

		}

		/// <summary>
		/// Cria a instancia com os dados do modelo de dados.
		/// </summary>
		/// <param name="dataModel"></param>
        /// <param name="uiContext"></param>
        /// <param name="entityTypeManager"></param>
		public $classname$($datamodeltype$ dataModel, string uiContext, $IEntityTypeManager$ entityTypeManager)
			: base(dataModel, uiContext, entityTypeManager)
		{

		}]]></Code>
    </Snippet>
