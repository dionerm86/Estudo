    </Header>
    <Snippet>
      <References />
      <Imports />
      <Declarations>
        <Literal Editable="false">
          <ID>classname</ID>
          <Type></Type>
          <ToolTip>Class Name</ToolTip>
          <Default>ClassNamePlaceholder</Default>
          <Function>ClassName()</Function>
        </Literal>
        <Literal Editable="true">
          <ID>datamodeltype</ID>
          <Type></Type>
          <ToolTip>Tipo do modelo de dados</ToolTip>
          <Default>DataModel</Default>
